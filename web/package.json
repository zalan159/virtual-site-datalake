{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:goview": "cd go-view && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:goview\"",
    "build": "tsc -b && vite build",
    "build:no-check": "vite build",
    "build:goview": "cd go-view && npm run build",
    "build:goview-safe": "cd go-view && npm install --ignore-scripts && npm run build",
    "build:all": "npm run build:goview-safe && npm run build:no-check",
    "build:all-check": "npm run build:goview && npm run build",
    "clean": "rimraf dist",
    "postbuild:all": "echo '两个项目已成功打包到同一个dist目录'",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-api": "orval"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/pro-components": "^2.8.7",
    "@babylonjs/core": "^8.5.0",
    "@babylonjs/loaders": "^8.5.0",
    "@cyntler/react-doc-viewer": "^1.17.0",
    "@orillusion/core": "^0.8.5-dev.6",
    "@types/moment": "^2.11.29",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.25.1",
    "axios": "^1.9.0",
    "babylonwander": "^1.5.0",
    "dashjs": "^5.0.2",
    "dayjs": "^1.11.13",
    "hls.js": "^1.6.2",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-player": "^2.16.0",
    "react-router-dom": "^7.5.2",
    "simple-peer": "^9.11.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "orval": "^7.9.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-cesium": "^1.2.23"
  }
}
